{
  "id": "caido-themes",
  "name": "Caido Themes",
  "version": "1.0.1",
  "description": "A plugin for managing and customizing themes in Caido",
  "author": {
    "name": "bebiks",
    "email": "bebiks@cvssadvisor.com",
    "url": "https://github.com/bebiks/caidothemes"
  },
  "plugins": [
    {
      "kind": "frontend",
      "id": "caido-themes-frontend",
      "name": "Caido Themes: Frontend",
      "entrypoint": "frontend/script.js",
      "style": "frontend/style.css",
      "backend": {
        "id": "caido-themes-backend"
      }
    },
    {
      "kind": "backend",
      "id": "caido-themes-backend",
      "name": "Caido Themes: Backend",
      "runtime": "javascript",
      "entrypoint": "backend/script.js"
    }
  ]
}
